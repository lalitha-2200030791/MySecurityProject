<!DOCTYPE html>
<html>
<head><title>Admin Dashboard</title></head>
<body>
    <h2>Admin Dashboard</h2>
    <table border="1">
        <tr><th>ID</th><th>Username</th><th>Email</th><th>Role</th><th>Action</th></tr>
        <% users.forEach(user => { %>
            <tr>
                <td><%= user.id %></td>
                <td><%= user.username %></td>
                <td><%= user.email %></td>
                <td><%= user.role %></td>
                <td><a href="/delete/<%= user.id %>">Delete</a></td>
            </tr>
        <% }); %>
    </table>
    <a href="/logout">Logout</a>
</body>
</html>
